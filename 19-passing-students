public Boolean isPassed(Boolean passedExam, Boolean passedAssignments, Boolean passedProject) {
    if((passedExam && passedAssignments) || (passedAssignments && passedProject) || (passedProject && passedExam)){
        return true;
    }
    else if(passedExam==false && passedAssignments==false || passedAssignments==false && passedProject==false || passedProject==false && passedExam==false){
        return false;
    }
    
    return true;
}